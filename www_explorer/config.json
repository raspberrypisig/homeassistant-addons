{
    "name": "WWW Explorer",
    "version": "0.4",
    "slug": "www_explorer",
    "url": "https://github.com/raspberrypisig/homeassistant-addons/www_explorer",
    "description": "Explore files and folders listing under /config/www directory from HTTP requests.",
    "arch": ["armhf", "armv7", "aarch64"],
    "startup": "application",
    "boot": "auto",
    "map": ["config:ro", "share:ro", "backup:ro", "addons:ro", "ssl:ro"],
    "options": {},
    "panel_icon": "mdi:wrap-disabled",
    "ports": {
        "8099/tcp": 8000
      },
    "ports_description": {
        "8099/tcp": "Web Server port"
    },
    "schema": {},
    "webui": "http://[HOST]:[PORT:8000]",
    "homeassistant_api": true,
    "full_access": false,
    "hassio_api": false,
    "ingress": true,
    "ingress_entry": "/homeassistant"
}
